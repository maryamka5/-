int main() {
  int a, b, c;
  
  // Ввод значений
  printf("Введите a: ");
  scanf("%d", &a);
  printf("Введите b: ");
  scanf("%d", &b);
  printf("Введите c: ");
  scanf("%d", &c);

  // Вычисление дискриминанта
  double d = pow(b, 2) - 4 * a * c;

  // Вывод результата
  if (d > 0) {
    printf("Два действительных корня\n");
  } else if (d == 0) {
    printf("Один действительный корень\n");
  } else {
    printf("Два комплексных корня\n");
  }

  return 0;
}

