C
#include <stdio.h>

int main() {
  double a, b, c, e, f, g, h;
  printf("Введите a: ");
  scanf("%lf", &a);
  printf("Введите b: ");
  scanf("%lf", &b);
  printf("Введите c: ");
  scanf("%lf", &c);
  printf("Введите e: ");
  scanf("%lf", &e);
  printf("Введите f: ");
  scanf("%lf", &f);
  printf("Введите g: ");
  scanf("%lf", &g);
  printf("Введите h: ");
  scanf("%lf", &h);
  double f1 = (a * a * a + g + b) / (c * (e * f - 3));
  printf("Значение функции f1: %lf\n", f1);
  double f2 = (a * a * a + g + b) / (c * (e * f - 3) * h);
  printf("Значение функции f2: %lf\n", f2);
  return 0;
}

