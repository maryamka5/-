  cout << "Введите b: ";
  cin >> b;
  cout << "Введите c: ";
  cin >> c;
  cout << "Введите e: ";
  cin >> e;
  cout << "Введите f: ";
  cin >> f;
  cout << "Введите g: ";
  cin >> g;
  cout << "Введите h: ";
  cin >> h;
  double f1 = (a * a * a + g + b) / (c * (e * f - 3));
  cout << "Значение функции f1: " << f1 << endl;
  double f2 = (a * a * a + g + b) / (c * (e * f - 3) * h);
  cout << "Значение функции f2: " << f2 << endl;
  return 0;
}

