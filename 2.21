C
#include <stdio.h>
#include <math.h>

double f(double e, double f, double g, double h) {
  return pow(e, 3) / (2 * b * c);
}

double b(double g, double h) {
  return sqrt(pow(g - h, 2) - 3 * sin(h));
}

double c(double g, double h) {
  return g - h;
}

int main() {
  double e, f, g, h, x;
  
  // Ввод значений
  printf("Введите e: ");
  scanf("%lf", &e);
  printf("Введите f: ");
  scanf("%lf", &f);
  printf("Введите g: ");
  scanf("%lf", &g);
  printf("Введите h: ");
  scanf("%lf", &h);
  printf("Введите x: ");
  scanf("%lf", &x);

  // Вычисление y
  double y = f(e, f, g, h) * pow(x, 2) + b(g, h) * x + c(g, h);

  // Вывод результата
  printf("y = %lf\n", y);

  return 0;
}

