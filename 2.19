C
#include <stdio.h>
#include <math.h>

int main() {
  double a, b;
  printf("Введите a: ");
  scanf("%lf", &a);
  printf("Введите b: ");
  scanf("%lf", &b);

  double u = sqrt(a * a + 25) / (sqrt(b) + a + b);
  double y = (fabs(a) + 2 * sin(b)) / (5.5 * a);

  printf("u = %lf\n", u);
  printf("y = %lf\n", y);

  return 0;
}
