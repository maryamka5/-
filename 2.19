C++
#include <iostream>
#include <cmath>

using namespace std;

int main() {
  double a, b;
  cout << "Введите a: ";
  cin >> a;
  cout << "Введите b: ";
  cin >> b;

  double u = sqrt(a * a + 25) / (sqrt(b) + a + b);
  double y = (abs(a) + 2 * sin(b)) / (5.5 * a);

  cout << "u = " << u << endl;
  cout << "y = " << y << endl;

  return 0;
}

