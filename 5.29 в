c (5.29 в)

#include <stdio.h>

int main() {
  int a, sum = 0;
  printf("Введите a: ");
  scanf("%d", &a);
  for (int i = a; i <= 200; i++) {
    sum += i;
  }
  printf("Среднее арифметическое: %lf\n", (double)sum / (200 - a + 1));
  return 0;
}

C++

#include <iostream>

using namespace std;

int main() {
  int a, sum = 0;
  cout << "Введите a: ";
  cin >> a;
  for (int i = a; i <= 200; i++) {
    sum += i;
  }
  cout << "Среднее арифметическое: " << (double)sum / (200 - a + 1) << endl;
  return 0;
}

python 

a = int(input("Введите a: "))
sum = 0
for i in range(a, 201):
  sum += i
print("Среднее арифметическое:", sum / (200 - a + 1))
